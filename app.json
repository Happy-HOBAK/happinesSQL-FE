{
  "expo": {
    "name": "HappinesSQL",
    "slug": "HappinesSQL",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/mainicon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/mainSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/mainicon_final.png",
        "backgroundColor": "#ffffff"
      },
      "usesCleartextTraffic": true
    },
    "web": {
      "favicon": "./assets/MainLogo.png"
    }
  }
}
