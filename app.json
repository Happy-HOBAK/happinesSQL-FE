{
  "expo": {
    "name": "HappinesSQL",
    "slug": "HappinesSQL",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/mainicon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/mainSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["remote-notification"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/mainicon_final.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hobak.happinessql",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDjIM_tUuKmZfNSnv6aFfv18K6OClnpnCY"
        }
      }
    },
    "web": {
      "favicon": "./assets/MainLogo.png"
    },
    "extra": {
      "eas": {
        "projectId": "2ffc2f6c-cf2a-4201-a430-9a5e9acb7124"
      }
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 34,
            "targetSdkVersion": 34,
            "buildToolsVersion": "34.0.0"
          },
          "ios": {
            "deploymentTarget": "13.4"
          }
        }
      ]
    ]
  }
}
